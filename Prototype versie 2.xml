<!-- We now ask for the farmers information, to ensure that the people of TreeAid
know where to find the farmer. -->
<form id="farmernameEN">
    <block>
     <prompt>
      <audio src="http://ict4d2021.saadittoh.com/group11/wavs/EN_please_say_your_name.wav"/>
     </prompt>
    </block>
    <property name="inputmodes" value="dtmf"/>
    <property name="interdigittimeout" value="2s"/>
    <property name="timeout" value="4s"/>

    <record name="messageName" beep="true" maxtime="5s" dtmfterm="true">
    <noinput>
     <audio src="http://ict4d2021.saadittoh.com/group11/wavs/EN_I_didnt_hear_anything_please_repeat.wav"/>
    </noinput>
    </record>

    <block>
     <prompt>
      <audio src="http://ict4d2021.saadittoh.com/group11/wavs/EN_please_say_the_name_of_the_village_where_you_live.wav"/>
     </prompt>
    </block>
    <property name="inputmodes" value="dtmf"/>
    <property name="interdigittimeout" value="2s"/>
    <property name="timeout" value="4s"/>

    <record name="messageVillage" beep="true" maxtime="10s" dtmfterm="true">
    <noinput>
     <audio src="http://ict4d2021.saadittoh.com/group11/wavs/EN_I_didnt_hear_anything_please_repeat.wav"/>
    </noinput>
    </record>

    <block>
        <data namelist="messageName messageVillage" src="http://ict4d2021.saadittoh.com/group11/scripts/script.php" enctype="multipart/form-data" method="post"/>
    </block>

     <block>
       <prompt>
        <audio src="http://ict4d2021.saadittoh.com/group11/wavs/EN_thank_you_for_your_help.wav"/>
       </prompt>
      <goto next="#goodbyeEN"/>
     </block>
</form>

